    "file-loader": "^6.2.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@google/dscc": "^0.3.22",
    "babel-preset-env": "^1.7.0",
    "react": "^17.0.2",
    "react-countup": "^6.4.0",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "react-spring": "^9.5.5",
    "yarn": "^1.22.19"
  },
